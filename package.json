{
  "name": "slack-discord-theme",
  "version": "1.0.0",
  "description": "Discord theme for slack",
  "main": "scripts/setup.js",
  "scripts": {
    "build-scss": "node-sass --include-path scss scss/main.scss out/main.css",
    "move-css": "node scripts/move-css.js",
    "watch-scss": "nodemon -e scss -x \"npm run build-scss && npm run move-css\"",
    "start": "node scripts/init-interop.js && npm run watch-scss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattco98/slack-discord-theme.git"
  },
  "keywords": [
    "slack",
    "discord",
    "theme",
    "slack-theme",
    "discord-theme",
    "dark",
    "dark-theme",
    "slack-discord-theme",
    "black",
    "black-theme"
  ],
  "author": "Matthew Olsson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattco98/slack-discord-theme/issues"
  },
  "homepage": "https://github.com/mattco98/slack-discord-theme#readme",
  "devDependencies": {
    "node-sass": "^4.9.0"
  },
  "dependencies": {
    "fs-extra": "^7.0.0",
    "nodemon": "^1.18.4"
  }
}
